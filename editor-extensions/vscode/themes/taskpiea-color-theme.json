{
    "$schema": "vscode://schemas/color-theme",
    "name": "Taskpiea",
    "type": "dark",
    "semanticHighlighting": true,
    "colors": {
        "titleBar.activeBackground": "#2F2D28",
        "sideBar.background": "#2F2D28",
        "sideBar.border": "#403f3a",
        "activityBar.background": "#2F2D28",
        "activityBar.border": "#403f3a",
        "statusBar.background": "#2F2D28",
        "editor.background": "#222222",
        "editor.foreground": "#D0D0D0",
        "focusBorder": "#9c6b17",
        "editorCursor.foreground": "#FFFFC8",
        "editorLineNumber.foreground": "#646464",
        "editorLineNumber.activeForeground": "#FFFFFF",
        "editor.selectionBackground": "#DCDC5A",
        "editor.selectionForeground": "#000000",
        "editor.lineHighlightBackground": "#3B2F26",
        "editor.wordHighlightBackground": "#55201488",
        "editor.findMatchBackground": "#5F5819",
        "editorBracketMatch.background": "#57572D",
        "editorBracketMatch.border": "#57572D"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#969696"
            }
        },
        {
            "name": "Defines",
            "scope": [
                "meta.preprocessor.define",
                "entity.name.function.preprocessor"
            ],
            "settings": {
                "foreground": "#aabe8c"
            }
        },
        {
            "name": "Type Names",
            "scope": [
                "entity.name.type"
            ],
            "settings": {
                "foreground": "#e7857c"
            }
        },
        {
            "name": "Namespaces",
            "scope": "entity.name.namespace",
            "settings": {
                "foreground": "#c8aadc"
            }
        },
        {
            "scope": "keyword, storage.type",
            "settings": {
                "foreground": "#73bedc"
            }
        },
        {
            "name": "Variables",
            "scope": "variable",
            "settings": {
                "foreground": "#88A8F8"
            }
        },
        {
            "name": "Functions",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#FFC882"
            }
        },
        {
            "scope": "entity.name.enum",
            "settings": {
                "foreground": "#b477b9"
            }
        },
        {
            "scope": "constant.other",
            "settings": {
                "foreground": "#E6AAC8"
            }
        }
    ]
}